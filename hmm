-- SkipEvent'i çalıştır
wait(10)

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local remoteEvent = ReplicatedStorage:WaitForChild("Remote"):WaitForChild("SkipEvent")

while true do
    remoteEvent:FireServer()
    wait(5)
end

-- AutoSkip fonksiyonunu çağır
local argsAutoSkip = {
    [1] = "AutoSkip"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Setting"):FireServer(unpack(argsAutoSkip))

-- x2 Speed fonksiyonunu çağır
local argsX2Speed = {
    [1] = "x1 Speed"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("x2Event"):FireServer(unpack(argsX2Speed))
